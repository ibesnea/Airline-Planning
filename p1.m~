%% Initialization
    addpath('/Users/Claudia/Documents/MSc-1/Q2/AE4423/Airline-Planning')
    clearvars
    clc
    close all
%% Input
    [d,airports] = distance('group11.xlsx');
    [C,Yieldk] = opcost('group11.xlsx');
    
    Nodes   = airports(2);          %Number of airports;
    acType  = k;
    
    %Remove infinite values of Yield and set them to zero for cases where 
    %i=j; 
    %Yield(find(isinf(Yield))) = 0;
%% Initiate CPLEX Model
%   Create model
        model                   =   'Problem1_Model';
        cplex                   =   Cplex(model);
        cplex.Model.sense       =   'maximize'; 

%   Decision variable
        DV                      =  Nodes*Nodes*(acType+2);
%% Objective Function 
        obj = reshape([ Yieldd ; Yieldd; reshape(C,3,Nodes*Nodes)],DV,1);